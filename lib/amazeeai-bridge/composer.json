{
    "name": "symfony/ai-amazeeai-platform",
    "description": "amazee.ai LiteLLM platform bridge for Symfony AI",
    "license": "MIT",
    "type": "symfony-ai-platform",
    "keywords": [
        "ai",
        "bridge",
        "amazeeai",
        "litellm",
        "platform"
    ],
    "authors": [
        {
            "name": "Christopher Jossart",
            "email": "christophe@colorfield.dev"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/ai-generic-platform": "^0.3",
        "symfony/ai-platform": "^0.3",
        "symfony/http-client": "^7.3|^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^12.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Symfony\\AI\\Platform\\Bridge\\AmazeeAi\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\AI\\Platform\\Bridge\\AmazeeAi\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "thanks": {
            "name": "symfony/ai",
            "url": "https://github.com/symfony/ai"
        }
    }
}
